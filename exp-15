#include <stdio.h>

int main()
{
    int start, size;

    printf("Enter starting block: ");
    scanf("%d", &start);

    printf("Enter file size (number of blocks): ");
    scanf("%d", &size);

    printf("Allocated blocks are:\n");
    for (int i = 0; i < size; i++) {
        printf("%d ", start + i);
    }

    return 0;
}
